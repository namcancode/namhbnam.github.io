<!DOCTYPE html>
<html>

<head>
  <title>ChatIO</title>
  <link rel="stylesheet" type="text/css" href="/fonts/fontawesomeProByNam.css">
  <link rel="stylesheet" type="text/css" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
  <link rel="stylesheet" type="text/css" href="https://namboss.blob.core.windows.net/huong/mdb.css">
  <link rel="stylesheet" href="lib/chessboardjs/css/chessboard-0.3.0.css">
  <link rel="stylesheet" type="text/css" href="/stylesheets/component.css" />

  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <div id="particles-js">
  </div>
  <div class="loginForm">
    <div class="fs-form-wrap" id="fs-form-wrap">
      <div id="myform" class="fs-form fs-form-full">
        <ol class="fs-fields">
          <li class="fs-current">
            <label class="fs-field-label fs-anim-upper active" for="txtUsername">What's your name?</label>
            <input class="fs-anim-lower" id="txtUsername" placeholder="Boss" type="text">
          </li>
        </ol>
      </div>
      <div class="fs-controls">
        <button id="btnRegister" class="fs-continue fs-show">Start</button>
      </div>
      <span class="fs-message-error"></span>
    </div>
  </div>

  <div class="card rare-wind-gradient chat-room">
    <div class="card-body row user-card width-100" id="chatForm">

      <!-- Grid row -->
      <div class="col col-lg-4 col-md-6 px-lg-2 px-2">

        <!-- Grid column -->
        <div class=" px-0">

          <h6 class="font-weight-bold mb-3 text-center text-lg-left">Member Online</h6>
          <div class="height-0 white z-depth-1 px-2 pt-3 pb-0 members-panel-1 scrollbar-light-blue">
            <ul class="list-unstyled friend-list">

            </ul>
          </div>

        </div>
        <!-- Grid column -->

        <!-- Grid column -->
        <div class=" px-0 pt-4">

          <div class="white chat-message">

            <ul class="list-unstyled chat-1 scrollbar-light-blue" id="listMessage">

            </ul>
            <div class="white">
              <div class="form-group basic-textarea">
                <textarea class="form-control pl-2 my-0" id="txtMessage" rows="3" placeholder="Type your message here..."></textarea>
              </div>
            </div>
            <button type="button" id="btnSend" class="btn btn-outline-pink btn-rounded btn-sm waves-effect waves-dark float-right">Send</button>

          </div>

        </div>
        <!-- Grid column -->

      </div>
      <div class="wrap--content col-lg-9">
        <div class="progress md-progress progress-slide" id="progress1">
          <div class="progress-bar bg-info" id="bar1" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0"
            aria-valuemax="100"></div>
        </div>
        <div class="chess--board">
        </div>
        <div class="progress md-progress progress-slide" id="progress2">
          <div class="progress-bar bg-info" id="bar2" role="progressbar" style="width: 100%" aria-valuenow="100" aria-valuemin="0"
            aria-valuemax="100"></div>
        </div>
      </div>
      <a class="btn-floating btn-lg purple-gradient buttonAI"><i>AI</i></a>
      <div class="boardAI">
        <div id="board" class="board"></div>
        <div class="info">
          <!-- Search depth: -->
          <select id="search-depth">
            <option value="1">1</option>
            <option value="2">2</option>
            <option value="3" selected>3</option>
            <option value="4">4</option>
            <option value="5">5</option>
          </select>

          <br>
          <span>Positions evaluated: <span id="position-count"></span></span>
          <br>
          <span>Time: <span id="time"></span></span>
          <br>
          <span>Positions/s: <span id="positions-per-s"></span> </span>
          <br>
          <br>
          <div id="move-history" class="move-history">
          </div>
        </div>
      </div>
      <!-- Grid row -->

    </div>

  </div>

  <script src="/javascripts/jquery-3.3.1.min.js"></script>
  <script src="/javascripts/jquery-ui-1.12.1/jquery-ui.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.13.0/umd/popper.min.js"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy"
    crossorigin="anonymous"></script>
  <script src="https://namboss.blob.core.windows.net/huong/mdb.min.js"></script>
  <!-- particle begin-->
  <script src="/javascripts/particles.min.js"></script>
  <script src="/javascripts/app.js"></script>
  <!-- particle end-->
  <script src="lib/chessboardjs/js/chess.js"></script>
  <script src="lib/chessboardjs/js/chessboard-0.3.0.js"></script>
  <script src="<%= url %>/socket.io/socket.io.js"></script>
  <script src="/javascripts/javascript.js" type="text/javascript"></script>
  <script src="/javascripts/script.js"></script>

</body>

</html>